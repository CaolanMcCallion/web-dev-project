<!DOCTYPE html>
<html>

<head>
  <title>Admin - Student Records</title>
</head>

<!-- Link to return to dashboard -->
<a href="/admin/dashboard" style="display: inline-block; margin-bottom: 10px;">Back to Dashboard</a>

<body>
  <h1>All Students</h1>
  <table border="1">
    <tr>
      <th>Student ID</th>
      <th>Name</th>
      <th>Entry Level</th>
      <th>Status</th>
    </tr>
    <% students.forEach(student=> { %>
      <tr>
        <td>
          <%= student.student_id %>
        </td>
        <td>
          <%= student.first_name %>
            <%= student.last_name %>
        </td>
        <td>
          <%= student.entry_level %>
        </td>
        <td>
          <%= student.status_study %>
        </td>
      </tr>
      <% }) %>
  </table>
</body>

</html>